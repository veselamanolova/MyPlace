@model MyPlace.Areas.Notes.Models.NotesViewModel
@foreach (var entity in Model.UserEntites)
{

   <a >@entity.</a>

}
<p>
    <partial name="_AddNotePartial" model="Model.ManageableNote" />
</p>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Category</th>
            <th>Text</th>
            <th>Date</th>
            <th>Is Completed</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var note in Model.Notes)
        {
            <partial name="_NotesPartial" model="note" />
        }
    </tbody>
</table>
